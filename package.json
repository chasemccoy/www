{
  "name": "@chasemccoy/www",
  "version": "2024",
  "scripts": {
    "build": "ENVIRONMENT=production yarn clean && concurrently \"eleventy\" \"yarn build:css\"",
    "watch": "eleventy --watch",
    "serve": "eleventy --serve",
    "start": "concurrently \"eleventy --serve --quiet\" \"yarn watch:css\"",
    "debug": "DEBUG=* eleventy",
    "watch:css": "sass --watch src/css:src/_includes/styles",
    "build:css": "sass src/css:src/_includes/styles",
    "clean": "rm -rf _site"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/chasemccoy/www.git"
  },
  "author": {
    "name": "Chase McCoy",
    "email": "hi@chsmc.org",
    "url": "https://chsmc.org"
  },
  "license": "MIT",
  "homepage": "https://github.com/chasemccoy/www",
  "dependencies": {
    "@11ty/eleventy": "3.1.2",
    "@11ty/eleventy-fetch": "4.0.1",
    "@11ty/eleventy-img": "3.1.8",
    "@11ty/eleventy-plugin-rss": "1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "5.0.0",
    "@date-fns/utc": "^2.1.0",
    "date-fns": "^4.1.0",
    "eleventy-plugin-embed-twitter": "1.4.0",
    "eleventy-plugin-youtube-embed": "1.10.2",
    "markdown-it": "14.1.0",
    "markdown-it-anchor": "8.6.7",
    "sass": "1.89.2",
    "slugify": "1.6.6"
  },
  "devDependencies": {
    "@netlify/plugin-lighthouse": "6.0.1",
    "concurrently": "9.2.0"
  }
}
